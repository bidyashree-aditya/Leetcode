class Solution {
    public boolean isPalindrome(int x) {
       int temp=0;
       int ori=x;
        int y=0;
        while(x>0)
        {
        
            temp=x %10;
            x=  x/10;

            y=y*10+temp;
        }
        if (y== ori)
        {
            return true;
        }
        else 
        return false;

        
        
    }
}
